pipeline {
agent any
       stages{ 
          stage ('BUILD') {
            steps{ 
              sh ''' 
               sleep 5
              echo "this is build stage"
                  }
                } 
           stage ('test') {
             parallel{ 
              stage('test in chrome') {
               steps{ 
                 sh '''
                  sleep 5
                 echo "testing in chrome"
                   }
                  } 
            stage('test in mop'){
              steps{ 
               sh '''
               sleep 5
               echo "testing in mop"
               }
              } 
            } 
         }
     }
}
